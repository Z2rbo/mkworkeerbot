{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/health/?",
      "dest": "https://mkworkeerbot.up.railway.app/health"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://mkworkeerbot.up.railway.app/api/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "https://mkworkeerbot.up.railway.app/uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
